	<queryString>
		<![CDATA[SELECT
     pessoa.`CPF` AS pessoa_CPF,
     pessoa.`DATANASCIMENTO` AS pessoa_DATANASCIMENTO,
     pessoa.`NOME` AS pessoa_NOME,
     pessoa.`TELEFONE` AS pessoa_TELEFONE,
     pessoa.`cod_cidade` AS pessoa_cod_cidade,
     cidade.`IDCidade` AS cidade_IDCidade,
     cidade.`NomeCidade` AS cidade_NomeCidade,
     cidade.`uf` AS cidade_uf
FROM
     `cidade` cidade INNER JOIN `pessoa` pessoa ON cidade.`IDCidade` = pessoa.`cod_cidade`
WHERE pessoa.`CPF` = $P{paramCPF}]]>
	</queryString>
