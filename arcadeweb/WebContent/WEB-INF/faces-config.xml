<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<application>
	</application>
	<converter>
		<converter-id>ThemaConverter</converter-id>
		<converter-class>br.com.warhjr.converter.ThemaConverter</converter-class>
	</converter>
	<navigation-rule>
		<display-name>protected/admin/template.xhtml</display-name>
		<from-view-id>/protected/admin/template.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>logoutOk</from-outcome>
			<to-view-id>/public/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>public/user/template.xhtml</display-name>
		<from-view-id>/public/user/template.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>logoutPrincOk</from-outcome>
			<to-view-id>/public/login.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>protected/admin/listUser.xhtml</display-name>
		<from-view-id>/protected/admin/listUser.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>novoUsuario</from-outcome>
			<to-view-id>/protected/admin/newUser.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
</faces-config>
